{
  "name": "visual-flux-frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev --turbo -p 4000",
    "build": "tsec --noEmit && next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "format": "prettier --write --config .prettierrc \"**/*.{js,jsx,ts,tsx,css,json,mjs,cjs,yml,yaml}\" --ignore-path .gitignore --ignore-path .prettierignore",
    "format:check": "prettier --check --config .prettierrc \"**/*.{js,jsx,ts,tsx,css,json,mjs,cjs,yml,yaml}\" --ignore-path .gitignore --ignore-path .prettierignore",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --ui --coverage.enabled=true",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:watch": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
